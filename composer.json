{
    "name": "alt-design/alt-ai",
    "description": "Alt AI addon, add AI-powered content assistance to your site",
    "license": "MIT",
    "type": "statamic-addon",
    "autoload": {
        "psr-4": {
            "AltDesign\\AltAi\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "statamic/cms": "^4.0|^5.0"
    },
    "extra": {
        "statamic": {
            "name": "Alt AI",
            "description": "Alt AI addon"
        },
        "laravel": {
            "providers": [
                "AltDesign\\AltAi\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": false
        }
    }
}
